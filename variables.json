{
  "name": "drupal",
  "trademark": "Drupal",
  "installpath": "/data/wwwroot/drupal",
  "compose_file": "docker-compose.yml",
  "fork_url": "https://github.com/docker-library/drupal",
  "env_file": ".env",
  "requirements":{
    "cpu_arch": "Linux x86-64, ARM 32/64, Windows x86-64, IBM POWER8, x86/i686",
    "cpu": "1",
    "memory": "1",
    "disk": "20",
    "swap": "2",
    "url": "https://www.drupal.org/docs/user_guide/en/install-requirements.html"
  },

  "services":[
    {
      "name": "drupal-server",
      "port": "9001",
      "description_cn": "浏览器访问 Drupal",
      "description_en": "Browser access to Drupal Document by http",
      "required": "Y"
    }
  ],

  "faqs": [
